<?xml version="1.0" encoding="utf-8"?>
<!-- Pressed state -->
<set xmlns:android="http://schemas.android.com/apk/res/android">
    <objectAnimator
        android:duration="100"
        android:propertyName="translationZ"
        android:valueTo="2dp"
        android:valueType="floatType" />
    <objectAnimator
        android:duration="0"
        android:valueTo="6dp"
        android:valueType="floatType" />
</set>
